{
  "productization": [
    {
      "property": "Code",
      "type": "String",
      "mandatory": "YES",
      "unique": "YES",
      "notes": "Product code, alphanumeric 5 chars max, identifies product."
    },
    {
      "property": "Name",
      "type": "String",
      "mandatory": "YES",
      "unique": "NO",
      "notes": "Product Name."
    },
    {
      "property": "Description",
      "type": "String",
      "mandatory": "YES",
      "unique": "NO",
      "notes": "Product Description."
    },
    {
      "property": "Packages",
      "type": "Array",
      "mandatory": "YES",
      "unique": "YES",
      "notes": "List of Product Packages and their ACL details."
    },
    {
      "property": "Package > _TTL",
      "type": "Number",
      "mandatory": "YES",
      "unique": "NO",
      "notes": "Package caching limit before reloading configuration."
    },
    {
      "property": "Package > code",
      "type": "String",
      "mandatory": "YES",
      "unique": "YES ",
      "notes": "Package Code, alphanumeric 5 chars max, identifies package in product."
    },
    {
      "property": "Package > name",
      "type": "String",
      "mandatory": "YES",
      "unique": "NO",
      "notes": "Package Name."
    },
    {
      "property": "Package > description",
      "type": "String",
      "unique": "YES",
      "mandatory": "NO",
      "notes": "Package Description."
    },
    {
      "property": "Package > acl",
      "type": "Object",
      "mandatory": "NO",
      "unique": "YES",
      "notes": "Package access level permissions on designated API Services."
    }
  ],
  "multitenancy": [
    {
      "property": "Code",
      "type": "String",
      "mandatory": "YES",
      "unique": "YES",
      "notes": "Tenant code, alphanumeric 4 chars max, identifies the tenant."
    },
    {
      "property": "Name",
      "type": "String",
      "mandatory": "YES",
      "unique": "NO",
      "notes": "Tenant Name."
    },
    {
      "property": "Description",
      "type": "String",
      "mandatory": "YES",
      "unique": "NO",
      "notes": "Tenant Description."
    },
    {
      "property": "Applications",
      "type": "Array",
      "mandatory": "YES",
      "unique": "YES",
      "notes": "List of Tenant Applications and their details."
    },
    {
      "property": "Applications > Application",
      "type": "Object",
      "mandatory": "YES",
      "unique": "YES",
      "notes": "Tenant Application that uses a product package with different keys and optional custom acl."
    },
    {
      "property": "Applications > Application > product",
      "type": "String",
      "mandatory": "YES",
      "unique": "YES",
      "notes": "Product code used by application."
    },
    {
      "property": "Applications > Application > package",
      "type": "String",
      "mandatory": "YES",
      "unique": "NO",
      "notes": "Package used by application."
    },
    {
      "property": "Applications > Application > description",
      "type": "String",
      "mandatory": "YES",
      "unique": "NO",
      "notes": "Application's description."
    },
    {
      "property": "Applications > Application > acl",
      "type": "Object",
      "mandatory": "NO",
      "unique": "YES",
      "notes": "Overrides default package ACL for this Application."
    },
    {
      "property": "Applications > Application > _TTL",
      "type": "Timestamp",
      "mandatory": "YES",
      "unique": "YES",
      "notes": "Caching limit of application before reloading its content in soajs."
    },
    {
      "property": "Applications > Application > Keys",
      "type": "Array",
      "mandatory": "YES",
      "unique": "YES",
      "notes": "Application Keys containing external api keys and service configuration."
    },
    {
      "property": "Applications > Application > Keys > Key",
      "type": "String",
      "mandatory": "YES",
      "unique": "YES",
      "notes": "Application Key Id."
    },
    {
      "property": "Applications > Application > Keys > config",
      "type": "Object",
      "mandatory": "YES",
      "unique": "YES",
      "notes": "Application Configuration of api services; Tenant Specific data goes here."
    },
    {
      "property": "Applications > Application > Keys > extKeys",
      "type": "Array",
      "mandatory": "YES",
      "unique": "YES",
      "notes": "Application external keys array"
    },
    {
      "property": "Applications > Application > Keys > extKeys > extKey",
      "type": "String",
      "mandatory": "YES",
      "unique": "YES",
      "notes": "Application external key Value; encrypted value of main application Key."
    },
    {
      "property": "Applications > Application > Keys > extKeys > expDate",
      "type": "Timestamp",
      "mandatory": "YES",
      "unique": "YES",
      "notes": "Application external key expiry Date."
    },
    {
      "property": "Applications > Application > Keys > extKeys > device",
      "type": "Object",
      "mandatory": "NO",
      "unique": "NO",
      "notes": "Application external key device security configuration."
    },
    {
      "property": "Applications > Application > Keys > extKeys > geo",
      "type": "Object",
      "mandatory": "NO",
      "unique": "NO",
      "notes": "Application external key geo location security configuration."
    }
  ]
}